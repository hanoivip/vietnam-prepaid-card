{
	"name" : "hanoivip/vietnam-prepaid-card",
	"type" : "library",
    "repositories" : [
		{
			"type" : "path",
			"url" : "../payment-method-contract",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../payment-method-tsr",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../events",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../shop",
			"options" : {
				"symlink" : true
			}
		}, {
			"type" : "path",
			"url" : "../payment-contract",
			"options" : {
				"symlink" : true
			}
		}
	],	
	"require" : {
		"php" : ">=7.0.0",
		"fideloper/proxy" : "~4.0",
		"laravel/framework" : ">=5.8",
		"laravel/tinker" : "~1.0",
		"hanoivip/payment-method-contract" : "dev-main",
		"hanoivip/payment-method-tsr": "dev-main",
		"hanoivip/events" : "dev-master",
		"hanoivip/payment-contract": "dev-main",
		"hanoivip/shop" : "dev-master"
	},
    "require-dev": {
        "filp/whoops": "~2.0",
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "~1.0",
        "phpunit/phpunit": "~6.0",
        "symfony/thanks": "^1.0"
    },
	"autoload" : {
		"psr-4" : {
			"Hanoivip\\VietnamPrepaidCard\\" : "src/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"Hanoivip\\VietnamPrepaidCard\\LibServiceProvider"
			]
		}
	}
}
